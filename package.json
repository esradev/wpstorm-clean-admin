{
  "name": "wpstorm-clean-admin",
  "version": "1.0.0",
  "description": "Wpstorm Clean Admin",
  "main": "index.jsx",
  "scripts": {
    "preview": "npm-run-all --parallel sync wpstart tailwindwatch",
    "sync": "browser-sync start -p 'payamito.abc' --files 'assets/build/*.js' 'assets/build/*.css'",
    "start": "npm-run-all --parallel wpstart tailwindwatch",
    "build": "npm-run-all --sequential wpbuild tailwindbuild",
    "wpstart": "wp-scripts start --output-path=assets/build",
    "wpbuild": "wp-scripts build --output-path=assets/build",
    "buildalpine": "wp-scripts build assets/js/alpine.js --output-path=assets/alpine",
    "startalpine": "wp-scripts start assets/js/alpine.js --output-path=assets/alpine",
    "tailwindbuild": "npx @tailwindcss/cli -i ./src/index.css -o ./assets/build/index.css --minify",
    "tailwindwatch": "npx @tailwindcss/cli -i ./src/index.css -o ./assets/build/index.css --watch --postcss --minify",
    "plugin-zip": "wp-scripts plugin-zip"
  },
  "files": [
    "assets/**",
    "includes/**",
    "languages/**",
    "vendor/**",
    "changelog.md",
    "readme.md",
    "wpstorm-clean-admin.php"
  ],
  "keywords": [],
  "author": "wpstorm",
  "license": "ISC",
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-direction": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
